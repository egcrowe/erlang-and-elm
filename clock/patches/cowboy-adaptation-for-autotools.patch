--- /dev/null
+++ b/configure.ac
@@ -0,0 +1,53 @@
+AC_PREREQ([2.69])
+AC_INIT([cowboy], [2.8.0], [essen@ninenines.eu])
+AC_CONFIG_SRCDIR([src/cowboy.erl])
+AM_INIT_AUTOMAKE([foreign])
+AM_SILENT_RULES([yes])
+
+# Checks for programs.
+AC_ERLANG_NEED_ERL
+AC_ERLANG_NEED_ERLC
+AC_PATH_PROG(DIALYZER, dialyzer)
+AC_PATH_PROG(CT_RUN, ct_run)
+AC_PATH_PROG(A2X, a2x)
+AC_PATH_PROG(ASCIIDECK, asciideck)
+
+# Checks for libraries.
+AC_ERLANG_SUBST_ERTS_VER
+erlang_erts_maj_ver=`echo $ERLANG_ERTS_VER | cut -d'.' -f1`
+if test $erlang_erts_maj_ver -lt 11; then
+  AC_MSG_ERROR([Erlang/OTP version must >= OTP-23.0!])
+fi
+AC_ERLANG_CHECK_LIB([erts], [],
+  [AC_MSG_ERROR([erts was not found!])])
+AC_ERLANG_CHECK_LIB([kernel], [],
+  [AC_MSG_ERROR([kernel was not found!])])
+AC_ERLANG_CHECK_LIB([stdlib], [],
+  [AC_MSG_ERROR([stdlib was not found!])])
+AC_ERLANG_CHECK_LIB([crypto], [],
+  [AC_MSG_ERROR([crypto was not found!])])
+cowlib_url=https://github.com/ninenines/cowlib.git
+AC_ERLANG_CHECK_LIB([cowlib], [],
+  [AC_MSG_ERROR([cowlib was not found! Please fetch from $cowlib_url, build and install it.])])
+ranch_url=https://github.com/ninenines/ranch.git
+AC_ERLANG_CHECK_LIB([ranch], [],
+  [AC_MSG_ERROR([ranch was not found! Please fetch from $ranch_url, build and install it.])])
+ct_helper_url=https://github.com/ninenines/ct_helper
+AC_ERLANG_CHECK_LIB([ct_helper], [],
+  [AC_MSG_WARN([ct_helper was not found! This is needed to run the test suites.
+  Please fetch from $ct_helper_url, build and install it.])])
+gun_url=https://github.com/ninenines/gun.git
+AC_ERLANG_CHECK_LIB([gun], [],
+  [AC_MSG_WARN([gun was not found! This is needed to run the test suites.
+  Please fetch from $gun_url, build and install it.])])
+asciideck_url=https://github.com/ninenines/asciideck
+AC_ERLANG_CHECK_LIB([asciideck], [],
+  [AC_MSG_ERROR([asciideck was not found! This is needed to generate man pages.
+  Please fetch from $asciideck_url, build and install it.])])
+
+AC_ERLANG_SUBST_INSTALL_LIB_SUBDIR([cowboy], [2.8.0])
+
+AC_CONFIG_FILES([
+  Makefile
+  ebin/cowboy.app])
+AC_OUTPUT
